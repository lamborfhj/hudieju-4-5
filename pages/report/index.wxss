.page {
  width: 100%;
}

.share {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10px 20px;
}

.share text:last-child {
  color: #79A7E7;
}

.title {
  text-align: center;
  font-size: 20px;
  font-weight: 700;
}

.table {
  /* border: 1px solid #dadada; */
  display:table; 
  border-right: 0;
  border-bottom: 0;
  width: 100%;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.time_01 {
  width: 16px;
  height: 16px;
  vertical-align: bottom;
  padding-bottom: 4px;
  padding-left: 10px;
}
.td_01 {
   width: 10%;
   font-size: 10px;
   line-height: normal;
   text-align: center;
   border: 1px solid #333;
   display: flex;
   flex-direction: column;
   align-items: center;
   justify-content: center;
}

.td_02 {
  width: 30%;
  font-size: 10px;
  line-height: normal;
  text-align: center;
  border: 1px solid #333;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.td_03 {
  width: 40%;
  font-size: 10px;
  line-height: normal;
  text-align: center;
  border: 1px solid #333;
}
.td_03 .td1{
   border-bottom: 1px solid #333;
   height: 17px;
   line-height: 17px;
}

.td_03 view:last-child {
  border: none;
}

.td_04 {
  width: 20%;
  font-size: 10px;
  line-height: normal;
  text-align: center;
  border: 1px solid #333;
    /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; */
}

.td_04 .td1 {
  border-bottom: 1px solid #333;
  height: 17px;
  line-height: 17px;
  text-decoration:underline;
  color:blue;
}

.td_04 view:last-child {
  border: none;
}

.tr .td {
  font-size: 10px;
  line-height: normal;
  text-align: center;
  border: 1px solid #333;
}

.tr .td .td1 {
  width: 100%;
  font-size: 10px;
  /* line-height: 20px; */
  text-align: center;
  border: 1px solid #333;
  height: 40px;
}

.tr1 {
  width: 100%;
  line-height: normal;
  display: flex;
  flex-direction: row;
  font-size: 10px;
  height: 17px;
  line-height: 17px;
}

.tr1 .td1 {
  width: 80%;
  border: 1px solid #333;
  text-align: center;
}

.picker {
  font-size: 14px;
}

.tr1 .td2 {
  width: 20%;
  line-height: normal;
  border: 1px solid #333;
  text-overflow: ellipsis;
  overflow: hidden;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}